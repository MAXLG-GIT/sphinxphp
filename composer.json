{
    "name": "maxlg-git/sphinxphp",
    "type": "library",
    "description": "Sphinx Search PHP 7.3+ API with PSR compliance",
    "keywords": ["sphinx","search","api"],
    "homepage": "http://sphinxsearch.com/",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Andrew Aksyonoff",
            "homepage": "http://sphinxsearch.com/"
        }
    ],
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^3.7.*",
        "php-coveralls/php-coveralls": "^2.2.0"
    },
    "autoload": {
        "psr-0": {
            "Sphinx": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
